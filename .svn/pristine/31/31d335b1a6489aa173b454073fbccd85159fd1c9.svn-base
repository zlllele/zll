<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<link href="${base}/css/css.css" type="text/css" rel="stylesheet" />
<title>首页 - ${site.shortName!site.siteName}</title>
<script src="${base}/js/jquery.js" type="text/javascript"></script>
<script src="${base}/js/front.js" type="text/javascript"></script>
<script type="text/javascript">
	$(function() {
		Cms.siteCount("${site.siteId}");
	});
</script>
</head>
<body>
	<div class="main">
		[#include "../include/top.html"/]
		
		<div class="mid">
			<div class="midleft">
				<img src="/www/img/index_69.jpg" width="219" height="148" class="js_img" />
				<span class="js_pro_name">无烟式烯烧嘴</span>
			</div>
			<div class="middle">
				<div class="tabbox">
					<div class="tabmenu">
						<ul>
						[@cms_channel channelId='53bcdbaaa8014dd09d7581beb57522ec']
							<li><a href="${tag_bean.url}">${tag_bean.name}</a></li>
						[/@cms_channel]
						</ul>
					</div>
					<div id="tabcontent">
						<ul class="cli">
							[@cms_content_list channelId='53bcdbaaa8014dd09d7581beb57522ec' count='6' typeId='1' ]
							[#list tag_list as content]
							<li><a href="${content.url}">${content.title}</a> ${content.releaseDate}</li>
							[/#list]
							[/@cms_content_list]
						</ul>
					</div>
				</div>
			</div>
			<div class="midright">
				<img src="/www/img/index_68.jpg" width="198" height="214" border="0"
					usemap="#Map" />
				<map name="Map" id="Map">
					<area shape="rect" coords="8,4,185,45" href="#" />
					<area shape="rect" coords="12,6,185,42" href="#" />
					<area shape="rect" coords="11,61,186,99" href="#" />
					<area shape="rect" coords="11,119,189,156" href="#" />
					<area shape="rect" coords="14,175,178,209" href="#" />
				</map>
			</div>
		</div>
		<div class="main_pro">
			<img src="/www/img/index_72.jpg" width="975" height="38" border="0"
				usemap="#Map2" />
			<map name="Map2" id="Map2">
				<area shape="rect" coords="895,4,965,31" href="#" />
			</map>
			<div class="scroll_product">
				<div id=demo style="overflow: hidden; width: 945px; padding: 5px 10px 0px 5px; margin-left: 5px; color: #606060;" align=center>
					<table border=0 align=center cellpadding=0 cellspacing=0 cellspace=0>
						<tr>
							<td valign=top bgcolor=ffffff id=marquePic1>
								<table width='100%' border='0' cellspacing='0'>
									<tr>
										[@cms_friendlink_list typeId='1' count='10' ]
										[#list tag_list as f]
										<td align="center" valign="top">
											<div style="width: 190px; height: 125px; padding-top: 4px;">
												<a href="${f.domain}" target="_blank"><img src="${f.logoPath}" width="170" height="115" /></a>
											</div> 
											<span style="display: block; width: 150px;"><a href="${f.domain}" target="_blank">${f.name}</a></span>
										</td>
										[/#list]
										[/@cms_friendlink_list]
									</tr>
								</table>
							</td>
							<td id=marquePic2 valign=top></td>
						</tr>
					</table>
				</div>
			</div>
		</div>
		[#include "../include/footer.html"/]
	</div>
</body>
</html>
