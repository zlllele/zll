<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN"
        "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<title>用户添加</title>
<script type="text/javascript" src="../../commons/js/include_ui.js?dm=10212527"></script>
<script type="text/javascript" src="../../jdgl/commons/js/include_ui_jdgl.js?dm=60"></script>
<script type="text/javascript" src="log_details.js?'+Math.random();+'"></script>
</head>
<body>
	<form action="" name="formUpdate" id="formUpdate">
		<table id="tbSave" class="table_add" cellpadding="0" cellspacing="0">
			<tr>
				<td class="tdclass_view" nowrap>用户：</td>
				<td class="tdclass_edit">
					<input type="text" class="textInput" name="user_name" value="" readonly="readonly"/>
				</td>
				<td class="tdclass_view" nowrap>时间：</td>
				<td class="tdclass_edit">
					<input type="text" class="textInput" name="create_time" value="" readonly="readonly"/>
				</td>
			</tr>
			<tr>
				<td class="tdclass_view" nowrap>标题：</td>
				<td class="tdclass_edit" colspan="3">
					<input type="text" class="textInput" name="title" value="" readonly="readonly" style="width: 397px;"/>
				</td>
			</tr>
			<tr>
				<td class="tdclass_view" nowrap>内容：</td>
				<td class="tdclass_edit" colspan="3">
					<textarea class="textInput" name="content" value="" readonly="readonly" style="width: 397px;height: 45px;"></textarea>
				</td>
			</tr>
			<tr>
				<td class="tdclass_view" nowrap>IP：</td>
				<td class="tdclass_edit" colspan:"3">
					<input type="text" class="textInput" name="ip" value="" readonly="readonly"/>
				</td>
			</tr>
		</table>
	</form>
</body>
</html>
