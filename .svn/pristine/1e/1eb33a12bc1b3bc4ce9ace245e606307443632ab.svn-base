*{margin:0px;padding:0px;}

A.A1:link { COLOR: #003366; TEXT-DECORATION: underline;}
A.A1:visited { COLOR: #003366; TEXT-DECORATION: underline}
A.A1:active  { COLOR: #124164; TEXT-DECORATION: underline}
A.A1:hover   { COLOR: #124164; TEXT-DECORATION: underline}

img{border:0px;}
body{
	background: url('bg.jpg') -200px -83px;;
}
#navPanel{
	width: 100%;
	height: 30px;
	padding-top:4px;
	font-size:13px;
	background: url('menu_top_bg.png') top left repeat-x;
}
#navMenu{
	float: left;
	white-space: nowrap;
	padding: 0px 20px;
	height: 26px;
	overflow:hidden;
}
#navMenu a
{
   float:left;
	overflow:hidden;
	color:#FFFFFF;
	text-decoration: none;
   padding-left:5px;
	height: 26px;
}
#navMenu a span{
   float:left;
   cursor:pointer;
	padding:3px 10px 3px 5px;
	height: 26px;
}
#navMenu a:hover
{
	color:#000000;
   font-weight:bold;
   background: url('menu_top_hover_l.png') no-repeat left top;
}
#navMenu a:hover span
{
   background: url('menu_top_hover_r.png') no-repeat right top;
}
#navMenu a.active
{
	color:#000000;
   font-weight:bold;
   background: url('menu_top_active_l.png') no-repeat left top;
}
#navMenu a.active span
{
   background: url('menu_top_active_r.png') no-repeat right top;
}