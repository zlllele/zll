body {font-size:12px; color:#000; margin:0; padding:0;background:#fff;}
html,body { height:100%;}
tr, td, div {padding:0px; margin:0px;}

A.white:link {color:#FFF; font-size:12px; text-decoration:none;}
A.white:visited {color:#FFF; font-size:12px; text-decoration:none;}
A.white:hover {
	color:#FFF;
	font-size:12px;
	text-decoration:underline;
	position:relative;
	top:1px;
}
A.blue12b:link {color:#0E8EDC; font-size:12px; font-weight:bold; text-decoration:none; font-family:Arial, Helvetica, sans-serif;}
A.blue12b:visited {color:#0E8EDC; font-size:12px; font-weight:bold; text-decoration:none; font-family:Arial, Helvetica, sans-serif;}
A.blue12b:hover { font-family:Arial, Helvetica, sans-serif;
	color:#0B75B5;
	font-size:12px;
	text-decoration:none;
	font-weight:bold;
}
