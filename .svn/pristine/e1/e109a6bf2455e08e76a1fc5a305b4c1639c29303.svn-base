body {background: url('bg2.jpg') 0 -144px;color:#FFFFFF;margin: 0px; padding: 0px;}
/* other */
img {border: none; vertical-align: middle;}

a:link{color: #0066cc; text-decoration: none;}
a:visited{color: #0066cc; text-decoration: none;}
a:hover {color: #333333; text-decoration: underline;}
a:active{color: #0066cc; text-decoration: underline;}

ul {
   list-style: none;
   margin: 0;
   padding: 0;
   border:1px #246191 solid;
   border-bottom:0px;
}

img{
   border:0px;
   width:16px;
   height:16px;
}

#body{
   width:100%;
   overflow-x:hidden;
   overflow-y:auto;
   clear:left;
   padding: 0px 4px 0px 4px;
}

#bottom{
   display:none;
}
#bottom_left{
}
#bottom_center{
}
#bottom_right{
}
.L1 a:link, .L1 a:visited{
   color: #444659;
   font-size:9pt;
   display: block;
   text-decoration: none;
   height: 24px;
   background: url("menu1_bg.png") repeat-x;
}

.L1 a:link span, .L1 a:visited span{
  background: url("ar.png") no-repeat center left;
  padding-left: 16px;
  height:24px;
  vertical-align:middle;
  _padding:7px 0px 0px 16px;
  cursor:pointer;
  width:100%;
   
}
.L1 a:hover{
   color: #000000;
   font-weight:bold;
   background:  url("menu1_bg.png") repeat-x;
   background-position: 0 -24px;
}
/*  一级菜单 active 效果  */
.L1 a.active, .L1 a:hover.active, .L1 a:active.active, .L1 a:visited.active{
   background:  url("menu1_bg.png") repeat-x;
   background-position: 0 -24px;
}
.L1 a:link.active span, .L1 a:hover.active span, .L1 a:active.active span, .L1 a:visited.active span{
   color: #000000;
   font-weight:bold;
   background: url("arrow_d.png") no-repeat left;
  background-position: 0 1px;
}

.moduleContainer{display:none;background:url("white_bg.png");border-bottom:1px #9AB3D1 solid;color: #000000;}
.moduleContainer .blank{line-height:30px;padding-left: 5px;}
.module {margin-bottom:10px;position:relative;}
.module .detail{position:absolute;top:7px;right:20px;z-index:100;cursor:pointer;}
.module .header:link,
.module .header:hover,
.module .header:active,
.module .header:visited
{color:#FFFFFF;padding-left: 5px;line-height:25px;background:url("list_hd_bg.png");font-weight:normal;}
.module .module_body{padding-left: 5px;}

a.page:link,
a.page:hover,
a.page:active,
a.page:visited{display:inline;background:none;text-decoration:underline;font-weight:normal;}
