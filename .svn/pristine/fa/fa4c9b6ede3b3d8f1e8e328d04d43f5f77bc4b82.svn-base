@charset "utf-8";

body {
	background-image: url(images/jdgl_login/bg.jpg);
	padding: 0;
	margin: 0;
	background-repeat: repeat-x;
	display: block;
	text-align: center;
	background-color: #269ED3;
}

#logo {
	background-attachment: scroll;
	background-clip: border-box;
	background-color: transparent;
	background-image: url(images/jdgl_login/bg1.jpg);
	background-origin: padding-box;
	background-position: left top;
	background-repeat: no-repeat;
	background-size: auto auto;
	margin-bottom: 0;
	margin-left: auto;
	margin-right: auto;
	margin-top: 0;
	position: relative;
	width: 1004px;
	height: 550px;
	text-align: left;
}

